# Environment Configuration
NODE_ENV=development
PORT=3000

# TCP Server Ports (one per protocol/device type)
TCP_PORT_HT02=5000
TCP_PORT_GT06=5001
TCP_PORT_CONCOX=5002
TCP_PORT_TELTONIKA=5003
TCP_PORT_QUECLINK=5004
TCP_PORT_GENERIC=5005

# UDP Server Ports
UDP_PORT_TELTONIKA=6000
UDP_PORT_GENERIC=6001

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Queue Names
QUEUE_GPS_DATA=gps:data:incoming
QUEUE_DEVICE_STATUS=device:status:updates

# Logging
LOG_LEVEL=info
LOG_FILE=logs/device-server.log

# Server Settings
MAX_CONNECTIONS=10000
CONNECTION_TIMEOUT=300000
HEARTBEAT_INTERVAL=60000
